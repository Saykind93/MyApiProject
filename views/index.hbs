<h1>Гугл карта для поиска страны</h1>

<div id="map" style="width:100%;height:400px;">

<script>
function myMap() {
  var mapCanvas = document.getElementById("map");
  var mapOptions = {
    center: new google.maps.LatLng(51.5, -0.2), zoom: 10
  };
  var map = new google.maps.Map(mapCanvas, mapOptions);
}
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA__e9Psl9UjyWKmlpDEQ5_dpVf2WXKzpE&callback=myMap" async defer></script>
